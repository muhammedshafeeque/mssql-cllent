{
  "name": "byzand",
  "productName": "Byzand MSSQL Client",
  "version": "1.0.0",
  "description": "A beautiful MSSQL database client application",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:linux": "electron-forge make --platform=linux --arch=x64",
    "make:exe": "electron-forge make --platform=win32 --arch=x64",
    "make:all": "electron-forge make --platform=all --arch=all",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "build:installers": "npm run make:all"
  },
  "keywords": [
    "mssql",
    "sql-server",
    "database",
    "client",
    "electron",
    "desktop",
    "development",
    "database-management"
  ],
  "author": {
    "name": "Muhammed Shafeeque P",
    "email": "shafeequekkv95@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron-forge/shared-types": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "electron": "38.1.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "typescript": "~4.5.4",
    "vite": "^5.4.20"
  },
  "dependencies": {
    "@types/mssql": "^9.1.8",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "electron-squirrel-startup": "^1.0.1",
    "mssql": "^11.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
